<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="com.hridoy.language.japani">

    <dist:module dist:instant="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".N5.Grama.N5ViewIntentGrama"
            android:label="Lesson Details View"
            android:parentActivityName=".N5.N5Activity"></activity>
        <activity
            android:name=".N5.Voca.N5ViewIntentVoca"
            android:label="Lesson Details View"
            android:parentActivityName=".N5.N5Activity" />
        <activity
            android:name=".N1.N1Activity"
            android:label="Nihongo Sou Matome N1"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".N2.N2Activity"
            android:label="Nihongo Sou Matome N2"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".N3.N3Activity"
            android:label="Nihongo Sou Matome N3"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".N4.N4Activity"
            android:label="Minna No Nihongo N4"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".N5.N5Activity"
            android:label="Minna No Nihongo N5"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>